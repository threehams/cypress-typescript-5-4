{
  "compilerOptions": {
    "allowJs": false,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": false,
    "strict": true,
    "target": "esnext",
    "resolveJsonModule": true,
  },
}
